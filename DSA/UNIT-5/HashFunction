#include <iostream>
using namespace std;

int hashFunction(int key, int tableSize) {
    return key % tableSize;   // simple division method
}

int main() {
    int tableSize = 10;

    int keys[] = {12, 44, 23, 39, 20, 16};
    int n = sizeof(keys) / sizeof(keys[0]);

    for (int i = 0; i < n; i++) {
        cout << "Key " << keys[i] << " â†’ Index: "
             << hashFunction(keys[i], tableSize) << endl;
    }

    return 0;
}
